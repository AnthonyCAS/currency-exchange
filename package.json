    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-param": "^1.0.3",
    "js-yaml": "^3.10.0",
    "jsonschema": "^1.2.2",
    "md5": "^2.2.1",
    "mongodb": "^3.0.2",
    "morgan": "^1.9.0",
    "n158": "^1.1.2",
    "q": "^1.5.1",
    "run-sequence": "^2.2.1",
