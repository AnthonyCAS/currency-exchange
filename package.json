{
  "name": "n158-seed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run server",
    "server": "npm run process -- src/processes/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "process": "babel-node --presets es2015",
    "postinstall": "npm run install-all && npm run build-all",
    "build-webapp": "cd src/processes/server/webapps/$WEBAPP && npm run build",
    "install-webapp": "cd src/processes/server/webapps/$WEBAPP && npm install",
    "build-webapp:access": "WEBAPP=access npm run build-webapp",
    "build-webapp:exchange": "WEBAPP=exchange npm run build-webapp",
    "install-webapp:access": "WEBAPP=access npm run install-webapp",
    "install-webapp:exchange": "WEBAPP=exchange npm run install-webapp",
    "install-all": "npm run install-webapp:exchange && npm run install-webapp:access",
    "build-all": "npm run build-webapp:exchange && npm run build-webapp:access"
  },
  "author": "",
